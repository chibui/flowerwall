        <h3 class="section-sub-heading text-center">handcrafted and without comprimises.</h3>
      </div>
    </div>
    {{# each category}}
      <div class="row no-gutter">
        {{# each this}}
        <div class="col-sm-4 col-md-4">
          <div class="hovereffect">
            <div class="gallery">
              <!-- <a href="/products/{{this._id}}" role="button"> -->
                <img class="img-responsive" src="{{ this.imagePath }}" alt="{{ this.title }}">
                <div class="overlay">
                  <h2>{{this.title}}</h2>
                  <a class="info" href="/products/{{this._id}}">View</a>
                </div>
              <!-- </a> -->
            </div>
          </div>
        </div>
        {{/each}}
